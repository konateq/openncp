<?xml version="1.0" encoding="UTF-8"?>
<serviceGroup>
    <service name="ClientConnectorService">
        <messageReceivers>
            <messageReceiver mep="http://www.w3.org/ns/wsdl/in-out" class="eu.epsos.pt.cc.ClientConnectorServiceMessageReceiverInOut"/>
        </messageReceivers>
        <parameter name="ServiceClass">eu.epsos.pt.cc.ClientConnectorServiceSkeleton</parameter>
        <parameter name="useOriginalwsdl">true</parameter>
        <parameter name="modifyUserWSDLPortAddress">true</parameter>
        <operation name="queryPatient" mep="http://www.w3.org/ns/wsdl/in-out" namespace="http://clientconnector.protocolterminator.openncp.epsos/">
            <actionMapping>urn:ehdsi:queryPatient</actionMapping>
            <outputActionMapping>urn:ehdsi:queryPatientResponse</outputActionMapping>
        </operation>
        <operation name="retrieveDocument" mep="http://www.w3.org/ns/wsdl/in-out" namespace="http://clientconnector.protocolterminator.openncp.epsos/">
            <actionMapping>urn:ehdsi:retrieveDocument</actionMapping>
            <outputActionMapping>urn:ehdsi:retrieveDocumentResponse</outputActionMapping>
        </operation>
        <operation name="sayHello" mep="http://www.w3.org/ns/wsdl/in-out" namespace="http://clientconnector.protocolterminator.openncp.epsos/">
            <actionMapping>urn:ehdsi:sayHello</actionMapping>
            <outputActionMapping>urn:ehdsi:sayHelloResponse</outputActionMapping>
        </operation>
        <operation name="queryDocuments" mep="http://www.w3.org/ns/wsdl/in-out" namespace="http://clientconnector.protocolterminator.openncp.epsos/">
            <actionMapping>urn:ehdsi:queryDocuments</actionMapping>
            <outputActionMapping>urn:ehdsi:queryDocumentsResponse</outputActionMapping>
        </operation>
        <operation name="submitDocument" mep="http://www.w3.org/ns/wsdl/in-out" namespace="http://clientconnector.protocolterminator.openncp.epsos/">
            <actionMapping>urn:ehdsi:submitDocument</actionMapping>
            <outputActionMapping>urn:ehdsi:submitDocumentResponse</outputActionMapping>
        </operation>
    </service>
</serviceGroup>

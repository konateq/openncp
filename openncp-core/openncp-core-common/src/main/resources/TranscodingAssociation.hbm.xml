<?xml version="1.0"?>
<!DOCTYPE hibernate-mapping PUBLIC "-//Hibernate/Hibernate Mapping DTD 3.0//EN"
        "http://www.hibernate.org/dtd/hibernate-mapping-3.0.dtd">
<hibernate-mapping>
    <class name="eu.europa.ec.sante.openncp.core.common.ihe.tsam.model.TranscodingAssociation" table="transcoding_association">
        <id name="id" type="long">
            <column name="id"/>
            <generator class="assigned"/>
        </id>
        <many-to-one name="targedConcept" class="eu.europa.ec.sante.openncp.core.common.ihe.tsam.model.CodeSystemConcept" fetch="join">
            <column name="target_concept_id"/>
        </many-to-one>
        <many-to-one name="sourceConcept" class="eu.europa.ec.sante.openncp.core.common.ihe.tsam.model.CodeSystemConcept" fetch="join">
            <column name="source_concept_id"/>
        </many-to-one>
        <property name="quality" type="java.lang.String">
            <column name="quality"/>
        </property>
        <property name="status" type="java.lang.String">
            <column name="status"/>
        </property>
        <property name="statusDate" type="java.util.Date">
            <column name="status_date"/>
        </property>
    </class>
</hibernate-mapping>

<?xml version="1.0"?>
<!DOCTYPE hibernate-mapping PUBLIC "-//Hibernate/Hibernate Mapping DTD 3.0//EN"
        "http://www.hibernate.org/dtd/hibernate-mapping-3.0.dtd">
<hibernate-mapping>
    <class name="eu.europa.ec.sante.openncp.core.common.tsam.model.CodeSystemVersion" table="code_system_version">
        <cache usage="read-only"/>
        <id name="id" type="long">
            <column name="id"/>
            <generator class="assigned"/>
        </id>
        <property name="fullName" type="java.lang.String">
            <column name="full_name"/>
        </property>
        <property name="localName" type="java.lang.String">
            <column name="local_name"/>
        </property>
        <property name="effectiveDate" type="java.util.Date">
            <column name="effective_date"/>
        </property>
        <property name="releaseDate" type="java.util.Date">
            <column name="release_date"/>
        </property>
        <property name="status" type="java.lang.String">
            <column name="status"/>
        </property>
        <property name="statusDate" type="java.util.Date">
            <column name="status_date"/>
        </property>
        <property name="description" type="java.lang.String">
            <column name="description"/>
        </property>
        <property name="copyright" type="java.lang.String">
            <column name="copyright"/>
        </property>
        <property name="source" type="java.lang.String">
            <column name="source"/>
        </property>
        <many-to-one name="previousVersion" class="eu.europa.ec.sante.openncp.core.common.tsam.model.CodeSystemVersion" lazy="proxy">
            <column name="previous_version_id"></column>
        </many-to-one>
        <many-to-one name="codeSystem" class="eu.europa.ec.sante.openncp.core.common.tsam.model.CodeSystem" foreign-key="FK_CODE_SYSTEM_ID">
            <column name="code_system_id"/>
        </many-to-one>
        <bag name="concepts" table="code_system_concept">
            <key column="code_system_version_id"/>
            <one-to-many class="eu.europa.ec.sante.openncp.core.common.tsam.model.CodeSystemConcept"/>
        </bag>
    </class>
</hibernate-mapping>

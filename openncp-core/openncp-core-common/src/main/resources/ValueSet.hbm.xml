<?xml version="1.0"?>
<!DOCTYPE hibernate-mapping PUBLIC "-//Hibernate/Hibernate Mapping DTD 3.0//EN"
        "http://www.hibernate.org/dtd/hibernate-mapping-3.0.dtd">
<hibernate-mapping>
    <class name="eu.europa.ec.sante.openncp.core.common.ihe.tsam.model.ValueSet" table="value_set">
        <id name="id" type="long">
            <column name="id"/>
            <generator class="assigned"/>
        </id>
        <property name="oid" type="java.lang.String">
            <column name="oid"/>
        </property>
        <property name="epsosName" type="java.lang.String">
            <column name="epsos_name"/>
        </property>
        <property name="description" type="java.lang.String">
            <column name="description"/>
        </property>
        <bag name="versions" inverse="true" table="value_set_version" lazy="true">
            <key>
                <column name="id"/>
            </key>
            <one-to-many class="eu.europa.ec.sante.openncp.core.common.ihe.tsam.model.ValueSetVersion"/>
        </bag>
        <many-to-one name="codeSystem" class="eu.europa.ec.sante.openncp.core.common.ihe.tsam.model.CodeSystem">
            <column name="parent_code_system_id"/>
        </many-to-one>
    </class>
</hibernate-mapping>
